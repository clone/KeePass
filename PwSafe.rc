        MENUITEM SEPARATOR
        MENUITEM "&Hide Passwords Behind Stars (***)", ID_VIEW_HIDESTARS
        , CHECKED
    END
    POPUP "&Extras"
    BEGIN
        MENUITEM "Password &Generator",         ID_EXTRAS_GENPW
    END
    POPUP "&?"
    BEGIN
        MENUITEM "Visit KeePass &Homepage",     ID_INFO_HOMEPAGE
