                                                ID_FILE_CHANGEMASTERPW
        MENUITEM SEPARATOR
        MENUITEM "&Print Complete Password List", ID_FILE_PRINT
        POPUP "Export Complete Database"
        BEGIN
            MENUITEM "&TXT Format...",              ID_EXPORT_TXT
            MENUITEM "&HTML Format...",             ID_EXPORT_HTML
            MENUITEM "&XML Format...",              ID_EXPORT_XML
            MENUITEM "&CSV Format...",              ID_EXPORT_CSV
